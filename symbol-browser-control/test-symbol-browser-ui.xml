		<cmd name="FormatUpdate" _label="Update" _tip="Update the whole tree" accel="*Shift**Control*u"/>
		<cmd name="FileOpen" _label="_Open Project" _tip="Open Project" accel="*Control*o"/>
		<cmd name="FileSave" _label="_Save Project" _tip="Save Project" accel="*Control*s"/>
		<cmd name="FileSetFile" _label="_Set Current File" _tip="Set Current File"/>
		<cmd name="FileExit" _label="_Exit" _tip="Exit"/>
