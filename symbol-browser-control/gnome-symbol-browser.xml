<Root>
	<commands>
		<cmd name="FormatUpdate" _label="Update Symbols" _tip="Update the symbol tree"/>
		<cmd name="GotoFileSymbol" _label="Goto Symbol" 
			pixtype="stock" pixname="gtk-jump-to" 
			_tip="Goto the displayed symbol"/>
	</commands>

	<menu>
		<submenu name="View" _label="_View">
			<menuitem name="FormatUpdate" verb=""/>
		</submenu>
	</menu>
	
	<dockitem name="SymbolBrowserToolbar" homogeneous="0" band_num="2"
			position="2" in_new_band="0" look="icon" relief="none">
		<control name="FileSymbolList" behavior="expandable"/>
		<toolitem name="GotoFileSymbol" verb="" _label="_Goto to symbol"/>
	</dockitem>

</Root>
