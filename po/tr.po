#: ../gdl/gdl-dock-item.c:1443
msgid "UnLock"
msgstr "Kilidi Kaldır"

#. Hide menuitem.
#: ../gdl/gdl-dock-item.c:1450
msgid "Hide"
msgstr "Gizle"

#. Lock menuitem
#: ../gdl/gdl-dock-item.c:1455
msgid "Lock"
msgstr "Kilitle"

