#: ../gdl/gdl-dock-item.c:1525
msgid "UnLock"
msgstr "AtSlēgt"

#. Hide menuitem.
#: ../gdl/gdl-dock-item.c:1532
msgid "Hide"
msgstr "Slēpt"

#. Lock menuitem
#: ../gdl/gdl-dock-item.c:1537
msgid "Lock"
msgstr "Slēgt"

#: ../gdl/gdl-dock-item.c:1810
#, c-format
