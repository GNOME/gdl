#, c-format
msgid "Dock #%d"
msgstr "Dok nr %d"

#: ../gdl/layout.ui.h:1
msgid "Dock items"
msgstr "Elementy doku"

#: ../gdl/layout.ui.h:2
msgid "Layout Managment"
msgstr "Zarządzanie układem"

#: ../gdl/layout.ui.h:3
msgid "Saved layouts"
msgstr "Zapisane układy"

#: ../gdl/layout.ui.h:4
